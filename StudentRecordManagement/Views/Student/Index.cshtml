@model List<Student>

<div>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Middle Name</th>
                <th>Last Name</th>
                <th>Phone</th>
                <th>Address</th>
                <th>Class</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var student in Model)
            {
                <tr>
                    <td>@student.FirstName</td>
                    <td>@student.MiddleName</td>
                    <td>@student.LastName</td>
                    <td>@student.Phone</td>
                    <td>@student.Address</td>
                    <td>@student.ClassName</td>
                </tr>
            }
        </tbody>
    </table>
</div>